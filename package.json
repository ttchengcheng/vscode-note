{
    "name": "note",
    "displayName": "note",
    "description": "",
    "version": "0.0.1",
    "publisher": "ttchengcheng",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.changeCase"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.changeCase",
                "title": "Change Selection Caps"
            }
        ],
        "keybindings": [
            {
                "command": "extension.changeCase",
                "key": "ctrl+f1",
                "mac": "cmd+f1",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "change-case": "^3.0.2"
    }
}
