{
    "name": "vscode-note",
    "displayName": "note",
    "description": "",
    "version": "0.0.1",
    "publisher": "ttchengcheng",
    "license": "MIT",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.changeCase",
        "onCommand:extension.stringTransform",
        "onCommand:extension.stringService"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.changeCase",
                "title": "Change Selection Cases"
            },
            {
                "command": "extension.stringService",
                "title": "Service based on Selection"
            },
            {
                "command": "extension.stringTransform",
                "title": "Transform Selection"
            }
        ],
        "keybindings": [
            {
                "command": "extension.changeCase",
                "key": "ctrl+shift+f1",
                "mac": "cmd+shift+f1",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.stringTransform",
                "key": "ctrl+shift+f2",
                "mac": "cmd+shift+f2",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.stringService",
                "key": "ctrl+shift+f8",
                "mac": "cmd+shift+f8",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/js-base64": "^2.3.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "change-case": "^3.0.2",
        "js-base64": "^2.5.0"
    }
}
